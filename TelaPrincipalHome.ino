// Método que atualiza os JComboBoxes para o item "0"
public void atualizar() {
    // Criação de um array com todos os JComboBox a serem atualizados
    JComboBox[] combos = {qtt1, qtt2, qtt3, qtt4, qtt5, qtt6, qtt7, qtt8, qtt9, qtt10, qtt11, qtt12};
    
    // Para cada JComboBox no array, garantimos que o valor 0 esteja na lista e seja selecionado
    for (JComboBox combo : combos) {
        // Verifica se o item 0 já existe no JComboBox
        boolean containsZero = false;
        for (int i = 0; i < combo.getItemCount(); i++) {
            if (combo.getItemAt(i).equals(0)) {
                containsZero = true;
                break;
            }
        }
        
        // Se o item 0 não existe, adiciona-o
        if (!containsZero) {
            combo.addItem(0); 
        }
        
        // Define o item 0 como o item selecionado
        combo.setSelectedItem(0);
    }
}


    private void calcularTotal() {
    try {
        double totalGeral = 0.0;
        
        List<JCheckBox> checkboxes = Arrays.asList(comprando1, comprando2, comprando3, comprando4, comprando5, comprando6, comprando7, comprando8, comprando9, comprando10, comprando11, comprando12);
        List<JLabel> precos = Arrays.asList(precoprod1, precoprod2, precoprod3, precoprod4, precoprod5, precoprod6, precoprod7, precoprod8, precoprod9, precoprod10, precoprod11, precoprod12);
        List<JComboBox<String>> quantidades = Arrays.asList(qtt1, qtt2, qtt3, qtt4, qtt5, qtt7, qtt7, qtt8, qtt9, qtt10, qtt11, qtt12);
        
        for (int i = 0; i < checkboxes.size(); i++) {
            if (checkboxes.get(i).isSelected()) {
                double preco = Double.parseDouble(precos.get(i).getText().replace(",", "."));
                int quantidade = Integer.parseInt(quantidades.get(i).getSelectedItem().toString());
                double total = preco * quantidade;
                totalGeral += total;
            }
        }
        
     campototal.setText(String.format("%.2f", totalGeral));
        double FRETE = 5;
        double totalComFrete = totalGeral + FRETE;
     campoTotal.setText(String.format("%.2f", totalComFrete));
        
    } catch (NumberFormatException e) {
        JOptionPane.showMessageDialog(null, "Erro ao calcular o total. Verifique os valores."+e );
    }
}


